{
  "name": "jenson/easy",
  "description": "test easyswoole",
  "type": "project",
  "require": {
    "easyswoole/easyswoole": "3.3.7",
    "ext-pcntl": "*",
    "easyswoole/http-client": "1.3.11",
    "easyswoole/sync-invoker": "dev-master",
    "easyswoole/hot-reload": "dev-master",
    "easyswoole/atomic-limit": "dev-master",
    "easyswoole/jwt": "dev-master",
    "easyswoole/validate": "dev-master",
    "easyswoole/orm": "dev-master",
    "ext-json": "*",
    "easyswoole/redis": "dev-master",
    "easyswoole/elasticsearch": "dev-master",
    "ext-mbstring": "*",
    "easyswoole/kafka": "dev-master",
    "php-amqplib/php-amqplib": "^2.12@dev"
  },
  "minimum-stability": "dev",
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer/"
    }
